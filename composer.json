{
    "name": "kanopi/drupal-starter",
    "description": "Project template for modern Drupal projects with composer and docksal",
    "license": "GPL-2.0+",
    "type": "project",
    "authors": [
        {
            "name": "Sean Dietrich <sean@kanopistudios.com>",
            "role": "Developer / Architect"
        },
        {
            "name": "Paul Sheldrake <paul@kanopi.com>",
            "role": "Trouble Maker"
        },
        {
            "name": "Jim Birch <jim@kanopi.com>",
            "role": "Custodian"
        }
    ],
    "require": {
        "bower-asset/bootstrap": "^5.3.0",
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^1.7.3",
        "drupal/access_unpublished": "^1.5",
        "drupal/admin_toolbar": "^3.5",
        "drupal/block_class": "^4.0",
        "drupal/block_field": "^1.0@RC",
        "drupal/clamav": "4.x-dev@dev",
        "drupal/core": ">=10.4",
        "drupal/core-composer-scaffold": "^11.1",
        "drupal/core-recommended": "^11.1",
        "drupal/core-vendor-hardening": "^11.1",
        "drupal/critical_css": "^1.19",
        "drupal/diff": "^1.1",
        "drupal/ds": "*",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/easy_responsive_images": "^1.4",
        "drupal/element_class_formatter": "^2.0@beta",
        "drupal/entity_browser": "*",
        "drupal/entity_reference_revisions": "*",
        "drupal/environment_indicator": "^4.0",
        "drupal/field_formatter": "3.x-dev@dev",
        "drupal/field_group": "*",
        "drupal/focal_point": "^2.1",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_login": "^2.1",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/google_tag": "^2.0",
        "drupal/imagemagick": "^4.0",
        "drupal/inline_entity_form": "^3.0.0@rc",
        "drupal/launch_checklist": "^1.1",
        "drupal/layout_options": "^1.4",
        "drupal/lite_youtube_embed": "^1.0",
        "drupal/media_entity_download": "^2.4",
        "drupal/media_file_delete": "^1.3",
        "drupal/media_library_edit": "^3.0",
        "drupal/menu_bootstrap_icon": "^1.0",
        "drupal/menu_link_attributes": "^1.5",
        "drupal/metatag": "*",
        "drupal/nomarkup": "^1.0",
        "drupal/pantheon_advanced_page_cache": "^2.0",
        "drupal/paragraphs": "*",
        "drupal/paragraphs_edit": "*",
        "drupal/pathauto": "^1.8",
        "drupal/publication_date": "^3",
        "drupal/quick_node_clone": "^1.16",
        "drupal/redirect": "^1.9",
        "drupal/redis": "^1.7",
        "drupal/remove_http_headers": "^2.1",
        "drupal/resource_hints": "1.x-dev",
        "drupal/sam": "^1.3",
        "drupal/scanner": "^2.0@beta",
        "drupal/scheduler": "*",
        "drupal/scheduler_content_moderation_integration": "^3",
        "drupal/schema_metatag": "*",
        "drupal/seckit": "^2.0",
        "drupal/security_review": "^3",
        "drupal/seo_checklist": "^5.2",
        "drupal/simple_sitemap": "^4",
        "drupal/site_audit": "^4.1",
        "drupal/sitemap": "^2.0",
        "drupal/token_or": "*",
        "drupal/tour": "^2",
        "drupal/tour_core": "^1",
        "drupal/trash": "^3",
        "drupal/twig_tweak": "^3.3",
        "drupal/ui_patterns": "^1.7",
        "drupal/ui_patterns_field_formatters": "*",
        "drupal/ui_patterns_field_group": "*",
        "drupal/ui_patterns_settings": "^2.2@alpha",
        "drupal/ui_skins": "^1.0@alpha",
        "drupal/ui_styles": "^1.7",
        "drupal/ui_suite_bootstrap": "^5.0@alpha",
        "drupal/ultimate_cron": "^2.0@beta",
        "drupal/unused_modules": "^1.5",
        "drupal/view_unpublished": "^1.1",
        "drupal/views_bulk_edit": "^3",
        "drupal/viewsreference": "^2.0@beta",
        "drush/drush": "^13",
        "ewcomposer/unpack": "dev-master",
        "joachim-n/composer-manifest": "^1.0",
        "kanopi/saplings-content-base-header": "^1",
        "kanopi/saplings-content-base-seo": "^1",
        "kanopi/saplings-full-html-editor": "^1",
        "kanopi/saplings-tests": "^1",
        "kanopi/saplings_child": "^1",
        "kanopi/saplings_paragraphs": "^1.0",
        "kanopi/shrubs": "^0.2.5",
        "npm-asset/lite-youtube-embed": "^v0.3.3",
        "oomphinc/composer-installers-extender": "^2.0.0",
        "pantheon-systems/drupal-integrations": "^10"
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^11.1",
        "drupal/default_content": "^2.0@alpha",
        "drupal/devel": "^5.1",
        "ergebnis/composer-normalize": "^2.45",
        "palantirnet/drupal-rector": "^0.20.3",
        "vincentlanglet/twig-cs-fixer": "^3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/woredeyonas/Drupal-Recipe-Unpack.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-vendor-hardening": true,
            "ergebnis/composer-normalize": true,
            "ewcomposer/unpack": true,
            "joachim-n/composer-manifest": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "rvtraveller/qs-composer-installer": true,
            "tbachert/spi": false
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "file-mapping": {
                "[web-root]/.htaccess": false,
                "[web-root]/robots.txt": {
                    "append": "assets/custom-robots.txt"
                },
                "[web-root]/sites/development.services.yml": false
            },
            "locations": {
                "web-root": "./web"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "recipes/{$name}": [
                "type:drupal-recipe"
            ],
            "tests/cypress/cypress/support/{$name}": [
                "type:cypress-support"
            ],
            "tests/cypress/cypress/e2e/{$name}": [
                "type:cypress-e2e"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/private/scripts/quicksilver/{$name}/": [
                "type:quicksilver-script"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "npm-asset",
            "bower-asset",
            "cypress-support",
            "cypress-e2e"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "[ ! -f rector.php ] && cp vendor/palantirnet/drupal-rector/rector.php . || true",
            "vendor/bin/phpcs --config-set installed_paths vendor/drupal/coder/coder_sniffer || true"
        ],
        "build-assets": [
            "composer install --optimize-autoloader --no-dev"
        ],
        "code-check": [
            "@phpstan",
            "@rector-modules",
            "@rector-themes",
            "@code-sniff"
        ],
        "code-fix": [
            "@code-fix-modules",
            "@code-fix-themes",
            "@rector-fix",
            "@lint-php"
        ],
        "code-fix-modules": "vendor/bin/phpcbf --ignore=*/node_modules/* --standard=Drupal,DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info,txt,md,yml web/modules/custom ",
        "code-fix-themes": "vendor/bin/phpcbf --ignore=*/node_modules/* --standard=Drupal,DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info,txt,md,yml web/themes/custom ",
        "code-sniff": [
            "@code-sniff-modules",
            "@code-sniff-themes"
        ],
        "code-sniff-modules": "vendor/bin/phpcs --ignore=*/node_modules/* --standard=Drupal,DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info,txt,md,yml web/modules/custom ",
        "code-sniff-themes": "vendor/bin/phpcs --ignore=*/node_modules/* --standard=Drupal,DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info,txt,md,yml web/themes/custom ",
        "lint-php": "find web/modules/custom web/themes/custom -not -path '*/node_modules/*' -name '*.php' -exec php -l {} \\;",
        "phpstan": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phpstan analyse --memory-limit=-1 web/modules/custom web/themes/custom"
        ],
        "prepare-for-pantheon": "DrupalProject\\composer\\ScriptHandler::prepareForPantheon",
        "rector-fix": [
            "@rector-fix-modules",
            "@rector-fix-themes"
        ],
        "rector-fix-modules": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/rector process web/modules/custom/"
        ],
        "rector-fix-themes": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/rector process web/themes/custom/"
        ],
        "rector-modules": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/rector process web/modules/custom/ --dry-run"
        ],
        "rector-themes": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/rector process web/themes/custom/ --dry-run"
        ],
        "twig-fix": [
            "@twig-fix-modules",
            "@twig-fix-themes"
        ],
        "twig-fix-modules": "vendor/bin/twig-cs-fixer lint --fix docroot/modules/custom",
        "twig-fix-themes": "vendor/bin/twig-cs-fixer lint --fix docroot/themes/custom",
        "twig-lint": [
            "@twig-lint-modules",
            "@twig-lint-themes"
        ],
        "twig-lint-modules": "vendor/bin/twig-cs-fixer lint docroot/modules/custom",
        "twig-lint-themes": "vendor/bin/twig-cs-fixer lint docroot/themes/custom"
    }
}
